.container
{
	width: 100%;
	height: auto;
}
.container .content
{
	width: 95%;
	height: auto;
	margin: 0 auto;
	padding: 0 auto;
	margin-top: 50rpx;
	border-radius: 5px;
}
.container .content .ti
{
	width: 100%;
	height: 50rpx;
	padding-left: 20rpx;
	border-left: 5rpx solid #AB956D;	
}
.container .content .fir
{
	width: 100%;
	height: 170rpx;
	display: flex;
	flex-direction: column;
	background-color: white;
	border-radius: 5px;
}
.container .content .sed
{
	width: 100%;
	height: 410rpx;
	display: flex;
	flex-direction: column;
	margin-top: 50rpx;
	background-color: white;
	border-radius: 5px;
}
.container .content .thir
{
	width: 100%;
	height: 170rpx;
	display: flex;
	flex-direction: column;
	margin-top: 50rpx;
	background-color: white;
	border-radius: 5px;
}
.container .content input
{
	width: 100%;
	border-bottom: 1rpx solid #f4f4f4;
	height: 80rpx;
	font-size: 32rpx;
	padding-left: 10rpx;

}
textarea 
{
	width: 100%;
	border-bottom: 1rpx solid #f4f4f4;
	font-size: 32rpx;
	padding-top: 10rpx;
	padding-left: 10rpx;

}
.container .content .btn
{
	width: 90%;
	margin-top: 100rpx;
	background-color: #AB956D;
	color: white;
}
