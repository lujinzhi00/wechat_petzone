.container
{
	width: 90%;
	height: auto;
	margin: 0 auto;
	padding: 0 auto;
}
.container .header
{
	width: 100%;
	height: 150rpx;
	background-color: white;
	margin-top: 60rpx;
	position: relative;
	border-radius: 5px;
}
.container .header .img
{
	width: 60rpx;
	height: 60rpx;
	position: absolute;
	left: 44rpx;
	top: 43rpx;
}
.container .img image
{
	width: 100%;
	height: 100%;
}
.container .header .info
{
	position: absolute;
	width: 80%;
	height: 100%;
	right: 0;
	line-height: 74rpx;
	padding-left: 18rpx;
	font-size: 30rpx;
}
.container .serv
{
	width: 100%;
	height: 250rpx;
	background-color: white;
	margin-top: 30rpx;
	border-radius: 5px;
	padding-top: 40rpx;
	padding-left: 30rpx;
	color: #949494;
	font-size: 30rpx;
}
.container .serv image
{
	width: 250rpx;
	height: 120rpx;
}
.container .serv .sinfo
{
	width: 100%;
	height: 120rpx;
	display: flex;
	margin-bottom: 20rpx;
}
.container .serv .sinfo .servt
{
	display: flex;
	flex-direction: column;
	padding-left: 30rpx;
	line-height: 55rpx;
}
.container .order
{
	width: 100%;
	height: 250rpx;
	margin-top: 40rpx;
	background-color: white;
	border-radius: 5px;
}
.container .order .to
{
	width: 100%;
	height: 100rpx;
	margin-bottom: 20rpx;
	line-height: 100rpx;
	padding-left: 30rpx;
}
.container .order picker
{
	width: 90%;
	border: 1rpx solid rgb(128,138,135,0.8);
	height: 60rpx;
	text-align: center;
	margin-left: 5%;
}
.container .order  text
{
	font-size: 30rpx;
	line-height: 60rpx;
}
.container .act
{
	width: 34%;
	height: 85rpx;
	background-color: #f17843;
	color: white;
	font-size: 30rpx;
	line-height: 56rpx;
	border-radius: 42rpx;
	margin-right: 40rpx;
	margin-top: 80rpx;
	text-align: center;
}