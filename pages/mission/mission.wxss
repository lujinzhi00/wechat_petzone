.container
{
	width: 95%;
	height:1000rpx;
	padding: 0 auto;
	margin: 0 auto;
}
.container .head
{
	width: 100%;
	height: 150rpx;
	position: relative;
	margin-top: 10rpx;
	background-color: white;
	border-radius: 5px;
}
.container .head image
{
	width: 120rpx;
	height: 120rpx;
	position: absolute;
	left: 20rpx;
	top: 15rpx;
}
.container .head .title
{
	width: 55%;
	height: 40%;
	position: absolute;
	left: 150rpx;
	padding-left: 20rpx;
	font-size: 32rpx;
	display: flex;
	align-items: center;
}
.container .head .rep
{
	position: absolute;
	right: 0;
	top: 0;
	width: 20%;
	height: 40%;
	font-size: 25rpx;
	text-align: center;
	line-height: 60rpx;
	padding-left: 40rpx;
	color: #00FF66;
}
.container .head .left
{
	width: 60%;
	height: 25%;
	position: absolute;
	top: 45%;
	left: 150rpx;
	padding-left: 20rpx;
	font-size: 30rpx;
}
.container .head .right
{
	width: 60%;
	height: 25%;
	position: absolute;
	top: 70%;
	left: 150rpx;
	padding-left: 20rpx;
	font-size: 30rpx;
}
.container .head .right .colo
{
	color: #ff4200;
}
.container .content 
{
	margin-top: 10rpx;
	background-color: white;
	width: 100%;
	height: 1100rpx;
	border-radius: 5px;
	padding-top: 10rpx;
}
.content .lead
{
	width: 100%;
	height: 50rpx;
	display: flex;
	align-items: center;
	padding-left: 20rpx;
	border-left: 8rpx solid #fdab14;
	font-size: 30rpx;
}
.content .warn
{
	width: 100%;
	height: 130rpx;
	background-color: #99FF99;
	position: relative;
	overflow: hidden;
	padding: 0 auto;
	margin: 0 auto;
	margin-top: 15rpx;
	margin-bottom: 15rpx;
	font-size: 28rpx;
	padding-top: 20rpx;
	line-height: 50rpx;
}
.content .warn image
{
	width: 50rpx;
	height: 50rpx;
	position: absolute;
	left: 10rpx;
	top: 40rpx;
}
.content .warn .tip
{
	width: 80%;
	height: 100%;
	padding: 0 auto;
	margin: 0 auto;
}
.content .body
{
	width: 100%;
	height: 700rpx;
}
.content .body .tip2
{	
	width: 100%;
	height: 50rpx;
	font-size: 30rpx;
	border-bottom: 1rpx solid #AB956D;
	padding-left: 20rpx;
}
.content .body .des
{
	margin: 0 auto;
	width: 100%;
	height: 650rpx;
	padding: 50rpx 20rpx;
	font-size: 30rpx;
	color: #5c5859;
}
.set
{
	width: 100%;
	height: 120rpx;
	position: fixed;
	bottom: 0;
	background-color: white;
	display: flex;
	align-items: center;
	justify-content: center;
}
.set .bt
{
	width: 80%;
	height: 60rpx;
	background-color: #f17843;
	border-radius: 20rpx;
	font-size: 30rpx;
	color: white;
	font-family: Kaiti;
	display: flex;
	align-items: center;
	justify-content: center;
}
.showy
{
	width: 400rpx;
	height: 150rpx;
	text-align: center;
	background-color: white;
	opacity: 0.7;
	z-index: 999;
	position: fixed;
	left: 24%;
	top: 41%;
	font-size: 50rpx;
	line-height: 140rpx;
	border-radius: 26rpx;

}
.hide
{
	display: none;
}
.show
{
	display: block;
}